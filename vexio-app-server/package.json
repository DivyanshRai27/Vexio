{
  "name": "vexio-app-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "migrate": "npx sequelize-cli db:migrate && npm run seed",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint ./src",
    "build": "npx babel src -d dist/src && npx babel models -d dist/models &&  npx babel config -d dist/config",
    "start:dev": "npm run lint && npm run migrate && nodemon --exec babel-node src/index.js",
    "start": "npm run lint && npm run build && npm run migrate && node ./dist/src/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.35.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
